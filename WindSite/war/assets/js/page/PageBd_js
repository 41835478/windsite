(function($) {
	$.widget("ui.pageBd", {
				/**
				 * 参数
				 */
				options : {},
				_create : function() {
				},
				/**
				 * Page-Bd初始化
				 */
				_init : function() {
					var self = this;
					var bd = self.element;// 内容区
					$('.J_TRegion', bd).each(function() {
								$(this).pageRegion();
							});
				},
				toSource : function(isValidate, isTemp) {
					var self = this;
				}
			});

})(jQuery);
